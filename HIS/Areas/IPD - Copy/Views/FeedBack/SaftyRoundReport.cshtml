
@{
    ViewBag.Title = "SaftyRoundReport";
    Layout = "~/Views/Shared/_layout.cshtml";
}
<script src="~/Areas/IPD/JsModule/FeedBack/SaftyRoundReport.js"></script>
<style>
    .sft-group {
        background: #dfe8f1;
    }

    #IPDPatientList .table > tbody > tr > td, #IPDPatientList .table > tbody > tr > th, #IPDPatientList .table > tfoot > tr > td, #IPDPatientList .table > tfoot > tr > th,
    #IPDPatientList .table > thead > tr > td, #IPDPatientList .table > thead > tr > th {
        font-size: 12px;
        padding: 4px 2px;
        line-height: 1.42857143;
        vertical-align: top;
        border-top: none;
    }

    .basic {
        background: #0cdf7e6b;
    }

        .basic:nth-child(1) {
            border-top: 5px solid white;
            border-bottom: 5px solid white;
        }

    .request {
        background: #f7e0e0;
    }

        .request:nth-child(1) {
            border-top: 5px solid white;
        }

    .ambulance {
        background: #fff4dc;
    }

    #tblEmployee tbody td {
        padding: 2px;
        border: 1px solid #ddd !important;
    }

    #tblAmbulanceMaster tbody tr
    .btnDoc {
        margin: 2px 0px;
        font-size: 18px;
        height: 17px;
        line-height: 17px;
    }

    .btn-add {
        border: none;
        padding: 6px;
        margin-left: 1px;
    }

    .btn-tbl {
        border: none;
        padding: 1px 9px;
        font-size: 13px;
        margin-bottom: 2px;
    }

    .flex {
        display: flex !important;
        align-items: center;
    }

    hr {
        margin-top: 35px;
        margin-bottom: 5px;
        border: 0;
        border-top: 1px solid #eee;
    }

    .custom-table {
        width: 100%;
        line-height: 29px;
        font-size: 12px;
    }

    .fileinput.input-group {
        display: flex;
    }

    .text-17 {
        font-size: 17px;
    }

    .btn-flat {
        padding: 2px;
        border: none;
    }

    .scroll {
        width: 100%;
        background: #fff;
        overflow: overlay;
        max-height: 450px;
        display: block;
        margin: 0 0 5px 0;
    }

    .panel-body {
        /*box-shadow: 1px 1px 10px #d2d2d2;*/
        border-radius: 10px;
        padding: 5px 3px 0px 3px;
        position: relative;
    }

    .note {
        list-style: disc;
        color: red;
        font-size: 11px;
        font-weight: bold;
        background: #f7f7f7;
        border: 1px solid #d86c6c;
    }

    .bn {
        margin: 0px;
        font-size: 10pt;
        padding: 0px 10px;
        border-radius: 0;
        text-align: left;
        height: 30px;
    }

    .bc {
        border: 1px solid #00a65a;
        border-radius: 0;
    }

    .section-feedback {
        border-top: 2px solid #f39c12;
        background: #fff;
        padding: 5px;
        border-radius: 2px;
        margin: -12px 0px;
    }

    .section {
        border-top: 2px solid #0076d0;
        background: #fff;
        padding: 5px;
        border-radius: 2px;
        margin: 9px 0px;
    }

        .section table {
            margin: 0;
        }

            .section table th {
                font-weight: bold;
            }

            .section table th, .section table td {
                font-size: 12px;
            }

    h3.modal-schedule {
        margin: 0;
        background: #2492b3;
        color: #ffffff;
        padding: 4px;
        font-size: 16px;
        border-bottom: 1px solid white;
    }

    #tblSchedule, #tblStatus {
        margin: 0;
        border: 1px solid #2492b3;
        min-height: 130px;
        max-height: 130px;
    }

    .hide {
        display: none;
    }

    .bcc {
        border: 1px solid #f39c12;
        border-radius: 0;
    }

    .btnRate {
        height: 28px;
        width: 30%;
        border-radius: 0;
        margin: 0;
    }

    .patient-list {
        border: 1px solid #e9e9e9;
        margin: 4px 0;
        background: #f3f3f3;
        padding: 3px;
    }

    #tblInfo tr th, #tblInfo tr td {
        padding: 1px;
        color: #4b5056;
        background-color: #d4ecfd;
    }

    .patient-list label {
        font-size: 11px;
    }

    .ratecircle {
        border: 2px solid #2f89e7;
        color: #302f2f;
        width: 30px;
        height: 30px;
        background: #eaffff;
        border-radius: 40px;
        line-height: 27px;
        text-align: center;
        font-size: 19px;
        font-weight: bold;
        cursor: pointer;
    }

    .ratecircle-head {
        border: 2px solid #e7c82f;
        color: #ffffff;
        width: 30px;
        height: 30px;
        background: #ff2d2d;
        border-radius: 40px;
        line-height: 27px;
        text-align: center;
        font-size: 19px;
        font-weight: bold;
        cursor: pointer;
    }

    #tblQuest thead th p {
        font-size: 8px;
        text-align: center;
        margin: 0;
    }

    .Inactive {
        /*opacity: 0.5;*/
        pointer-events: none;
        cursor: not-allowed;
    }

    .overAllRate {
        width: 40px;
        background: #1569e7;
        height: 40px;
        border-radius: 30px;
        font-size: 18px;
        text-align: center;
        padding: 4px;
    }

    #tblQuest tbody tr:nth-child(even) th {
        border-bottom: 1px solid #fff;
    }

    #IPDPatientList {
        background: #ddd;
        min-height: 460px;
        overflow: auto;
        padding: 0px 5px;
    }
</style>
<div id="page-content">
    <div class="container">
        <div class="row">
            <div class="col-md-10">
                <div class="panel">
                    <div class="panel-heading" style="background-color:#0076d0;padding:2px;">
                        <h4 class="text-white">Safty Round Report</h4>
                    </div>
                    <div class="panel-body" style="background:#fff;padding:8px 20px;">
                        <div class="row">
                            <div class="col-md-3">
                                <label>Date</label>
                                <input type="date" id="txtDate" class="form-control" />
                            </div>
                            <div class="col-md-3" style="margin-top:3px;">
                                <label></label>
                                <select class="form-control" id="ddlFloor">
                                    <option>Select</option>
                                </select>
                            </div>
                            <div class="col-md-3">
                                <button class="btn-success" onclick="OpenReport();" style="margin-top:22px;height:25px;"><i class="fa fa-download"></i>&nbsp;Safty Round Report</button>
                            </div>
                            <div class="col-md-3">
                                <button class="btn-success" onclick="PatientAllotmentReport();" style="margin-top:22px;height:25px;"><i class="fa fa-download"></i>&nbsp;Staff Allotment Report</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>

