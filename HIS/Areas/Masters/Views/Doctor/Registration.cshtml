<div class="modal fade" id="RateModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog modal-xl" style="width: 900px !important;" role="document">
        <div class="modal-content">
            <div class="modal-header" style="padding: 5px 15px;background: #009688;color:#fff">
                <div style="display:flex">
                    <h4 class="modal-title" id="myModalLabel">Add Rate &nbsp;&nbsp;&nbsp;&nbsp;</h4>
                    <button id="btnLoad" class="btn btn-success btn-xs" type="button" onclick="return OnLoadGetRateRoomVisit()">LOAD</button>
                    &nbsp;&nbsp;&nbsp;&nbsp;<h4 class="modal-title"><span class="txtModalDocName"></span></h4>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                </div>
            </div>
            <div class="modal-body" style="padding: 5px 10px;">
                <div class="row">
                    <div class="col-md-5">
                        <div class="panel panel-primary">
                            <div class="panel-heading"><h5>Rate List</h5></div>
