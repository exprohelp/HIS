                                    <table class="table table-bordered" id="tblPanels">
                                        <thead>
                                            <tr>
                                                <th style="width:1%"><input type="checkbox" id="chkallPanels" class="checkbox" /></th>
                                                <th>Panels</th>
                                                <th style="width:1%">Action</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="col-md-7">
                        <div class="panel panel-primary">
                            <div class="panel-heading"><h5>Visits</h5></div>
                            <div class="panel-body">
                                <div class="table-responsive" style="height:227px;overflow:auto;overflow-y:scroll">
